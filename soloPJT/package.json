		"mocha": "^10.4.0",
		"nodemon": "^3.1.0",
		"pg": "^8.11.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"vite": "^5.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
